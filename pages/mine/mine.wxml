<!-- 订单 -->
  <view class="DingDan topo">
    <text class="tit">订单管理</text>
    <view class="lieBi">
      <view class='itemClass' bindtap='goOrder' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/order.png' ></image>
        <text class='itemText'>我的订单</text>
      </view>
       <view class='itemClass' bindtap='goShopList' data-types='1'>
        <image class='itemImg' src='../../images/rmb.png' ></image>
        <text class='itemText'>我买过的店</text>
      </view>
      <view class='itemClass' bindtap='goToAddress' data-types=''>
        <image class='itemImg' src='../../images/place.png' ></image>
        <text class='itemText'>我的地址</text>
      </view>
      <view class='itemClass' bindtap='goShopList' data-types='2'>
        <image class='itemImg' src='../../images/payInshop.png' ></image>
        <text class='itemText'>浏览过的店</text>
      </view>
    </view>
  </view>
  <view class="DingDan imgUrl" >
   <image src="http://www.shanxingniao.com/imgServer/images/use_test/qita/wodetiaofu.jpg" bindtap='jifen'></image>
  </view>
  <view class="DingDan">
    <text class="tit">店铺管理</text>
    <view class="lieBii">
      <view class='itemClass' bindtap='openB' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/guanli.png' ></image>
        <text class='itemText'>管理我的店</text>
      </view>
       <view class='itemClass' bindtap='openShop' data-types=''>
        <image class='itemImg' src='../../images/manage.png' ></image>
        <text class='itemText'>我要开店</text>
      </view>
      <view class='itemClass' bindtap='' data-types=''>
        <text style='font-weight:500;font-size:32rpx;color:red;'>{{invitatuonNum}}</text>
        <text class='itemText'>我的邀请码</text>
      </view>
    </view>
  </view>
  <view class="DingDan bottom">
    <text class="tit">关于我们</text>
    <view class="lieBie" style="border:none;">
      <view class='itemClassg' bindtap='aboutGs' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/about.png' ></image>
        <text class='itemText'>关于公司</text>
      </view>
       <view class='itemClassg' bindtap='zhaoshang' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/zhaoshang.jpg' ></image>
        <text class='itemText'>生态园招商</text>
      </view>
       <view class='itemClassg' bindtap='money' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/shoukuan.png' ></image>
        <text class='itemText'>收款码</text>
      </view>
       <view class='itemClassg' bindtap='signFn' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/peixun.png' ></image>
        <text class='itemText'>培训报名</text>
      </view>
    </view>
  </view>



