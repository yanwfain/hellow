<!-- pages/mine/mine.wxml -->
<view class='mineBox'>
  <image class='bgImg' src='../../images/minebg.png'></image>
  <!-- <image class='avatar' src='{{userInfo.avatarUrl}}'></image> -->
  <!-- <text class="nickName">{{userInfo.nickName}}</text> -->
  <view class='userInfor'>
    <!--  -->
    <button wx:if="{{wxuser==null}}" open-type='getUserInfo' class='getUser' bindgetuserinfo='getWxUserInfo'>获取基本用户信息</button>
    <image class='userImg' src='{{wxuser.avatarUrl}}'></image>
    <text class='userName'>{{wxuser.nickName}}</text>
    
  </view>
  <view class='nav'>
      <view class='itemClass' bindtap='goSign' data-types='{{item.types}}'>
        <image class='itemImgq' src='../../images/qd.png' ></image>
        <text class='itemText'>签 到</text>
      </view>
      <view class='itemClass' bindtap='goGold' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/gold.png' ></image>
        <text class='itemText'><text style='font-weight:400;font-size:28rpx;'>{{balance?balance:0}}</text></text>
      </view>
      <view class='itemClass' bindtap='goMyCollection' data-types='{{item.types}}'>
        <image class='itemImg' src='../../images/phone.png' ></image>
        <text class='itemText'>收 藏</text>
      </view>
  </view>
   

</view>
