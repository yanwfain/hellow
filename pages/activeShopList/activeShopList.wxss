/* pages/goJionList/goJionList.wxss */

.main{
  padding-top: 90rpx
}

/*
  搜索框
*/
.sea{
  background: white;
  display: flex;
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #e5e5e5;
  position: fixed;
  top: 0;
  left: 0;
  width: 92%;
}
.int{
  width: 100%;
}
.search{
  width: 100%;
  height: 54rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  color: #999999;
  background:rgba(201,200,200,0.16);
}
.searImg{
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx
}

.listItem{
  margin-top: 20rpx;
  background: white;
  padding: 20rpx 50rpx;
}

.listItem:last-child{
  margin-bottom: 20rpx;
}

.lable{
  margin-bottom: 32rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.lable text{
  color: #333333;
  font-size: 32rpx;
}

.lable image{
  width: 20rpx;
  height: 35rpx;
  display: block;
}

.content{
  display: flex;
  flex-direction: column;
}

.content image{
  display: block;
  width: 100%;
  height: 377rpx;
  margin: 20rpx 0 20rpx;
}

.content text:nth-of-type(1){
  font-size: 26rpx;
  color: #999999;
}

.content text:last-of-type{
  font-size: 28rpx;
  color: #333333;
}