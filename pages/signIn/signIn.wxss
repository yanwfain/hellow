.mineBox{
  width: 100vw;
  height: 346rpx;
  text-align: center;
  background: linear-gradient(to top, rgba(254, 141, 135, 1),rgba(254, 102, 98, 1));
}
.fonts{
  font-size: 38rpx;
  color: #fff;
  font-weight: 600;
}
.photoImg{
  width: 150rpx;
  height: 150rpx;
  border-radius: 250rpx;
  margin: 20rpx auto;
}
.signInCon{
  width: 700rpx;
  height: 40rpx;
  border-radius: 40rpx;
  border: 1rpx solid #ff5c61;
  margin: 50rpx auto;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.signIn{
  height: 37rpx;
  width: 97rpx;
  position: relative;
}
.signInLeft{
  border-radius:  37rpx 0 0 37rpx;
}
.signInright{
  border-radius: 0 37rpx 37rpx 0;
}
.signInBack{
  background: #ff5c61;
}
.sign{
  width: 80rpx;
  padding: 5rpx;
  line-height: 36rpx;
  font-size: 28rpx;
  border-radius: 36rpx;
  background: #ff5c61;
  color: #fff;
  position: absolute;
  left: 10rpx;
  bottom: -55rpx;
  text-align: center;
}
.signBack{
  background: #999;
  width: 100rpx;
}
.cont1{
  background: rgb(219, 217, 217);
   margin-top: 80rpx;
}
.cont{
  width: 750rpx;
  display: flex;
  height: 80rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #999;
 
}
.conts{
  width: 750rpx;
  display: flex;
  height: 100rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #999;
}
.cont view{
  padding: 20rpx 10rpx;
  font-size: 26rpx;
  text-align: center;
}
.contName{
  align-items: center;
  display: flex;
}
.contName text{
  width: 250rpx;
  text-align: left;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.contN{
  align-items: center;
  display: flex;
  justify-content: space-between;
}
.contN text{
  width: 165rpx;
  text-align: center;
  display: block;
}
.tishi{
  font-size: 17rpx;
}
.contN image{
  width: 30rpx;
  height: 50rpx;
}
.contName image{
  width: 40rpx;
  height: 40rpx;
  border-radius: 30rpx;
  margin-right: 10rpx;
}
.times{
  width: 300rpx;
}
.red{
  color: red;
  font-weight: 600;
}
.green{
  color: green;
}