<view>
  <!-- 轮播 -->
  <import src='../travelTemplate/travelTemplate.wxml'/>
  <template is="banner" data="{{banner,classData,Bind}}"></template>
  <!-- 导航 -->
  <view class='funNav commWidth'>
    <view class='navView' wx:for='{{navViewList}}' wx:key='index'>
      <image bindtap='goTravelOfficials' data-code='{{item.id}}' data-ty='{{item.ty}}' data-true='true' data-scenicCode='{{scenicCode}}'src='{{item.imgUrl}}' data-url='{{item.url}}' bindtab="linshi"></image>
      <view>{{item.title}}</view>
    </view>
  </view> 
  <!-- 分类导航 -->
  <import src='../travelTemplate/travelTemplate.wxml'/>
  <template is="classNav" data="{{navList,navTitle,iconState}}"></template>
  <!-- 旅游列表 -->
  <view class='classTitle'>— {{navTitle}}精选 —</view>
  <import src='../travelTemplate/travelTemplate.wxml'/>
  <template is="travelList" data="{{offcialList,entrance}}"></template>
</view>