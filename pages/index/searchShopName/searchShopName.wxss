@import '../../template/template.wxss';
page{
  background:#f2f2f2;
}
.sea{
  background: #fff;
  display: flex;
  padding: 17rpx 30rpx 17rpx 30rpx;
  border-top:1rpx solid #e5e5e5;
}
.seek{
  font-size: 32rpx;
  color: #333;
  width: 15%;
  line-height: 54rpx;
  text-align: center;
  font-weight: 400;
}
.int{
  width: 100%;
}
.search{
  width: 65%;
  height: 54rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  color: #666;
  background:rgba(201,200,200,0.16);
}
.searImg{
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx
}
/* 排序 */
.paixuClass{
  display: flex;
  padding: 10rpx 30rpx 10rpx;
}
.tabT{
  color: #666666;
  font-size: 26rpx;
  padding: 8rpx 15rpx;
  border-radius:28rpx;
  display: flex;
  flex: 1;
  justify-content: center;
}
.selectTabClass{
  padding: 8rpx 15rpx;
  background:rgba(255,92,97,0.2097);
  border-radius:28rpx;
  color: #FF5C61;
  font-size: 26rpx;
  display: flex;
  flex: 1;
  justify-content: center;
}
/*
  店目录
*/
.colorB{
  color:#1d60a7;
  font-size:20rpx;
}
.shop3{
  color: #1d60a7;
  font-size: 20rpx;
  padding: 10rpx;
}
.st{
  width: 450rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.kk{
  margin: 5rpx 0;
   width: 240rpx;
  border-radius: 3px;
  border: 1rpx solid rgb(253, 153, 147);
  font-size:20rpx;
  color:#FE8D87;
  text-align: center
}
.shopList{
  display: flex;
  flex-direction: column;
}
.shopItem{
  display: flex;
  padding: 20rpx 40rpx;
  border-top: 1rpx solid #f4f4f4;
  margin-top: 15rpx;
}
.shopItem:last-child{
  border-bottom: none;
}
.shopImg{
  /* width: 154rpx;
  height: 154rpx; */
  width: 130rpx;
  height: 130rpx;
  margin-right: 22rpx;
  border-radius: 6rpx;
  border: 1px solid #f2f2f2;
}
.shopInfo{
  width: 400rpx;
  display: flex;
  justify-content: space-between;
  flex: 1;
}
.infoText{
  display: flex;
  flex-direction: column;
  flex: 1;
  flex-wrap: wrap;
}
.up{
  display: flex;
  justify-content: space-between;
}
.shopName{
  font-size: 30rpx;
  margin-top: 5rpx;
  color: #333333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 400rpx;
  font-weight: 600;
}
.Imgs{
  width: 518rpx;
  display: flex;
  justify-content: space-around;
}
.Dimg{
  width: 145rpx;
  font-weight: 400;
}
.Dimg image{
  width: 145rpx;
  height: 145rpx;
  border: 1rpx solid #f2f2f2;
}
.DimgT1{
  width: 145rpx;
  display: block;
  font-size: 24rpx;
  margin: 5rpx 0 10rpx 0;
  color: #666;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.DimgT2{
  font-size: 26rpx;
  color: #CC3333;
  display: block;
  width: 45rpx;
}
