.money{
  display: flex;
  width: 495rpx;
  color: #666;
  font-weight: 400;
  font-size: 24rpx;
  position: relative;
}
.moneys{
  margin: 0 0 11rpx 0;
  height: 30rpx;
  width: 495rpx;
  color: #666;
  font-weight: 400;
  font-size: 24rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.wire{
  padding-right: 16rpx;
  margin-right: 24rpx;
  border-right: 1rpx solid #e5e5e5;
}
.Mright{
  position: absolute;
  right: 0;
}
.clo{
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  margin-right: 14rpx;
  background: #FF5C61;
  border-radius: 3px;
  color: #fff;
  font-size: 24rpx;
  text-align: center;
}
.shopStatus{
  padding: 10rpx;
  background: linear-gradient(to  right, #FE8D87, #FF6662);
  border-radius:6rpx;
  color: white;
  font-size: 18rpx;
  text-align: center;
}
.statu{
  font-size: 20rpx;
}
.location{
  font-size: 24rpx;
}

/*near  */
.near{
  display: flex;
  align-items: center;
  flex-direction: column;
  padding-top: 273rpx;
  width: 100%;
}
.nearShop{
  width: 370rpx;
  height: 343rpx;
  margin-bottom: 79rpx;
}
.nearText{
  font-size: 32rpx;
  color: #999999;
}
/* 二次 */
.rank{
  padding: 50rpx 30rpx 26rpx 30rpx;

  width: 690rpx;
  height: 40rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #666;
  font-weight: 400;
  border-bottom: 1px solid #e5e5e5;
}
.rankS{
  display: flex;
}

.rankS image{
  width: 12rpx;
  height: 6rpx;
  margin: auto 0;
  margin-left: 10rpx;
}
.fontW{
  font-weight: 600;
}

/* 搜索标签 */
.title{
  display: flex;
  align-items: center;
  width: 94%;
  margin: 25rpx auto;
  font-size: 30rpx;
}
.title text{
  line-height: 50rpx;
}
.title image{
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.content{
  display: flex;
  align-items: center;
  width: 94%;
  margin: 20rpx auto;
  font-size: 30rpx;
  flex-wrap: wrap;
  /* border-collapse:collapse; */
}
.alne{
  width: 24.5%;
  line-height: 80rpx;
  text-align: center;
  margin:0px 0px -1px -1px;
  border: 1rpx solid #e6e5e5;
  background: #fff;
  font-size: 24rpx;
  color: #666;
}
/* 全城&&附近 */
.scope{
  width: 20%;
  line-height: 54rpx;
}
.picker{
  font-size: 28rpx;
}