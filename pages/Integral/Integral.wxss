.topIntegral{
  width: 96%;
  background: white;
  text-align: center;
  padding-top: 15rpx;
  margin:0 auto;
  height: 400rpx;
  overflow: hidden;
}
.topIntegral image{
  width:100%;
  height: 100%; 
}
.yellow{
  background: rgb(255, 217, 0);
   position: fixed;
  bottom: 0;
  left:0;
  color:white;
  text-align: center;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 36rpx;
}
.center{
  position:absolute;
  top:40%;
  left:30%;
