<view class='title'>
  <view class="titleTxt {{index == id?'color':''}}" bindtap='goTxt' wx:for='{{navList}}' data-index='{{index}}'>{{item.name}}</view>
</view>
<!-- 内容 -->

<view class='content'>
  <view class='titleh' style='font-size:34rpx;'>{{content.name}}</view>
  <view class='titleh' wx:if='{{content.abstracts}}'>{{content.abstracts}}</view>
  <view class='titleh' style='text-align:left;' wx:if='{{content.tishi}}'>{{content.tishi}}</view>
  <block wx:for="{{content.textArr}}" wx:key>
    <view class='titleh' style='text-align:left;'>{{item.title}}</view>
    <text class='txt'>{{item.text}}</text>
  </block>
  <view class='titleh' style='text-align:right;margin-top:20rpx;'>{{content.contacts}}</view>
</view>
<view style='width:100rpx;height:200rpx;' wx:if="{{id != 0}}">

</view>